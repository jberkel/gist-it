    <uses-sdk android:minSdkVersion="7"/>
    <application>
        <uses-library android:name="android.test.runner"/>
        <activity android:name=".MainActivity" android:label="@string/app_name">
           <intent-filter>
               <action android:name="android.intent.action.MAIN"/>
